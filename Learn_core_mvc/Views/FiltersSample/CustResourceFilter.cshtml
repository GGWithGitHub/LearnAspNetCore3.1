
@{
}

<button onclick="OnClick()">Click on me</button>

<div id="dvResult">

</div>

@section Scripts{
    <script>
        function OnClick() {
            $.ajax({
                type: 'GET',
                url: '@Url.Action("GetLargeData", "FiltersSample")',
                success: function (response) {
                    if (response.status) {
                        $("#dvResult").empty().append(response.data);
                    }
                },
                error: function (err) {
                    alert('An error occurred: '+err);
                }
            });
        }
    </script>
}
